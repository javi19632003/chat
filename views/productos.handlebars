<div class="row">
    <div class="col">
        <h1>Productos</h1>
    </div>
</div>
<div class="row">
    <div class="col">
        <table class="table table-striped table-hover">
            <thead class="thead-dark">
                <tr>
                    <th>ID</th>
                    <th>TITULO</th>
                    <th>PRECIO</th>
                    <th>URL</th>
                </tr>
            </thead>
            <tbody>
            {{#each .}}
                    {{#if title}}
                    <tr>
                        <td>{{id}}</td>
                        <td>{{title}}</td>
                        <td>{{price}}</td>
                        <td>{{thumbail}}</td>
                    </tr>
                    {{/if}}       
            {{/each}}
            </tbody>
        </table>
    </div>
</div>
<div class="row">
    <div class="col">
        <a href="/" class="btn btn-outline-danger">Carga de Productos</a>
    </div>
</div>